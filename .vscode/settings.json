{
    "spellright.language": [
        "en_GB"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "latex-workshop.latex.outDir": "%DIR%/../build/",
    "latex-workshop.latex.tools": [ 
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "--shell-escape",
                "%DOC%"
            ],
            "env": {
                "openout_any": "a"
            }
        },
    ]
}